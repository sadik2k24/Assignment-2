public class Person {
    private String name;
    private int age;
    private String country;

    public Person(String name, int age, String country) {
        this.name = name;
        this.age = age;
        this.country = country;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public String getCountry() {
        return country;
    }

    public void setCountry(String country) {
        this.country = country;
    }

    public static void main(String[] args) {
        Person p1 = new Person("Alice", 30, "USA");
        System.out.println("Initial Name: " + p1.getName());
        
        p1.setAge(31);
        p1.setCountry("Canada");
        
        System.out.println("Updated Age: " + p1.getAge());
        System.out.println("Updated Country: " + p1.getCountry());
    }
}
